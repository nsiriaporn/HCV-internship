### Baseline characteristics
# Gender
table(HCW.Questionnaires$GENDER)
# Age group
hist(HCW.Questionnaires$AGE)
table(HCW.Questionnaires$AGE<30) # less than 30 yo
table(HCW.Questionnaires$AGE>29 & HCW.Questionnaires$AGE<51) # 30-50 yo
table(HCW.Questionnaires$AGE>50) # more than 50 yo
# Place of residence
table(HCW.Questionnaires$LOCALIZATION)
# Marital status
table(HCW.Questionnaires$MARITAL_STATUS)
# Profession
table(HCW.Questionnaires$PROFESSION)
